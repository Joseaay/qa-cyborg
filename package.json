{
    "name": "qa-cyborg",
    "version": "1.0.0",
    "description": "A GitHub bot APP for blocking PRs until QA has been passed and approved",
    "author": "Jose Ant. Ayllon <joseayllonllamas@hotmail.com>",
    "license": "ISC",
    "repository": "https://github.com/Joseaay/qa-cyborg.git",
    "homepage": "https://github.com/Joseaay/qa-cyborg",
    "bugs": "https://github.com/Joseaay/qa-cyborg/issues",
    "keywords": [
        "probot",
        "github",
        "probot-app"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "nodemon --exec \"npm start\"",
        "start": "probot run ./lib/index.js",
        "lint": "standard **/*.ts --fix",
        "test": "jest && standard **/*.ts",
        "test:watch": "jest --watch --notify --notifyMode=change --coverage"
    },
    "dependencies": {
        "js-yaml": "^3.13.1",
        "newrelic": "^6.1.0",
        "probot": "^9.0.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.0",
        "@types/js-yaml": "^3.12.1",
        "@types/nock": "^11.1.0",
        "@types/node": "^12.0.0",
        "eslint-plugin-typescript": "^0.14.0",
        "jest": "^24.0.0",
        "nock": "^11.6.0",
        "nodemon": "^1.17.2",
        "smee-client": "^1.0.2",
        "standard": "^12.0.1",
        "ts-jest": "^24.0.0",
        "typescript": "^3.3.1",
        "typescript-eslint-parser": "^22.0.0"
    },
    "engines": {
        "node": ">= 8.3.0"
    },
    "standard": {
        "parser": "typescript-eslint-parser",
        "env": [
            "jest"
        ],
        "plugins": [
            "typescript"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    }
}
